{
    "meta": {},
    "theme": {
        "aside": {
            "border-radius": "15px",
            "border-style": "solid",
            "border-width": "1px"
        },
        "code": {
            "border-radius": "15px",
            "border-style": "solid",
            "border-width": "0px",
            "indentationWidth": 4
        },
        "color": {
            "aside-deprecated": "var(--color-figure-orange)",
            "aside-deprecated-background": "var(--color-fill-orange-secondary)",
            "aside-deprecated-border": "var(--color-figure-orange)",
            "aside-experiment": "var(--color-figure-purple)",
            "aside-experiment-background": "var(--color-fill-purple-secondary)",
            "aside-experiment-border": "var(--color-figure-purple)",
            "aside-important": "var(--color-figure-yellow)",
            "aside-important-background": "var(--color-fill-yellow-secondary)",
            "aside-important-border": "var(--color-figure-yellow)",
            "aside-note": "var(--color-figure-light-gray)",
            "aside-note-background": "var(--color-fill-light-gray-secondary)",
            "aside-note-border": "var(--color-figure-light-gray)",
            "aside-tip": "var(--color-figure-teal)",
            "aside-tip-background": "var(--color-fill-teal-secondary)",
            "aside-tip-border": "var(--color-figure-teal)",
            "aside-warning": "var(--color-figure-red)",
            "aside-warning-background": "var(--color-fill-red-secondary)",
            "aside-warning-border": "var(--color-figure-red)",
            "button-background": "var(--color-fill-blue)",
